		"wrapper-webpack-plugin": "^2.1.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.2.2",
		"astro-eslint-parser": "file:..",
		"eslint": "^8.14.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-scope": "^7.0.0",
